{
  "name": "@mbari/react-ui",
  "version": "1.0.0",
  "private": true,
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "scripts": {
    "start": "yarn tailwind & yarn storybook --port 5001",
    "build": "rm -rf ./dist && tsc && yarn build-tailwind",
    "build-tailwind": "npx tailwindcss -i ./src/css/base.css -o ./dist/mbari-ui.css",
    "build-test": "rm -rf ./dist && tsc -m CommonJS",
    "tailwind": "npx tailwindcss -i ./src/css/base.css -o ./dist/mbari-ui.css  --watch",
    "storybook": "start-storybook -p 5000",
    "prettier": "prettier --config ../../.prettierrc 'src/**/*.{js,jsx,ts,tsx}' --write",
    "test:watch": "jest --watch --json --outputFile=.jest-test-results.json",
    "test:generate-output": "jest --maxWorkers=4 --json --outputFile=.jest-test-results.json || true",
    "test": "yarn jest --maxWorkers=4",
    "prebuild:storybook": "npm run test:generate-output",
    "build:storybook": "build-storybook -c .storybook -o build/",
    "predeploy": "npm run build:storybook",
    "deploy": "gh-pages -d build/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@date-io/luxon": "1.x",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/pro-duotone-svg-icons": "^5.15.4",
    "@fortawesome/pro-light-svg-icons": "^5.15.4",
    "@fortawesome/pro-regular-svg-icons": "^5.15.4",
    "@fortawesome/pro-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@hookform/resolvers": "^2.8.2",
    "@material-ui/core": "^4.12.4",
    "@material-ui/pickers": "^3.3.10",
    "@mbari/utils": "^1.0.0",
    "@sumocreations/forms": "^1.0.2",
    "clsx": "^1.1.1",
    "immutability-helper": "^3.1.1",
    "luxon": "^2.3.1",
    "next": "^12.1.4",
    "react": "^17.0.2",
    "react-dnd": "^15.1.1",
    "react-dnd-html5-backend": "^15.1.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.17.5",
    "react-select": "^5.1.0",
    "tailwindcss": "^3.0.7",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@storybook/addon-a11y": "^6.4.22",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-controls": "^6.4.22",
    "@storybook/addon-docs": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-jest": "^6.4.22",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-viewport": "^6.4.22",
    "@storybook/builder-webpack5": "^6.4.22",
    "@storybook/manager-webpack5": "^6.4.22",
    "@storybook/node-logger": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@sumocreations/scaffolds": "^1.0.12",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^26.0.15",
    "@types/luxon": "^2.3.0",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/uuid": "^8.3.4",
    "autoprefixer": "^9",
    "babel-jest": "^27.5.1",
    "babel-loader": "8.1.0",
    "css-loader": "^6.5.1",
    "jest": "^27.5.1",
    "postcss": "^8",
    "postcss-cli": "^9",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "prettier-plugin-tailwindcss": "^0.1.7",
    "resolve-url-loader": "^5.0.0",
    "storybook-addon-designs": "^6.2.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.1.2"
  }
}
